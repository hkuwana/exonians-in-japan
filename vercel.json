{
  "buildCommand": null,
  "cleanUrls": true,
  "trailingSlash": false,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/cilley",
      "destination": "/cilley.html"
    },
    {
      "source": "/updates",
      "destination": "/updates.html"
    },
    {
      "source": "/ja",
      "destination": "/ja.html"
    },
    {
      "source": "/unsubscribe",
      "destination": "/unsubscribe.html"
    }
  ]
}
